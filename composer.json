{
    "name": "vxm/yii2-desktop-notifier",
    "description": "Desktop notifier for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","desktop-notifier","console"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": ">=2.0.13",
        "jolicode/jolinotif": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "vxm\\desktopNotifier\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "bootstrap": "vxm\\desktopNotifier\\Bootstrap"
    }
}
